    "test": "cross-env NODE_ENV=test MONGO_DB_NAME=taskmanager_test mocha --require test/setup.js test/**/*.test.js --timeout 20000 --exit",
    "test:watch": "cross-env NODE_ENV=test MONGO_DB_NAME=taskmanager_test mocha --require test/setup.js test/**/*.test.js --watch --timeout 20000",
    "test:coverage": "cross-env NODE_ENV=test MONGO_DB_NAME=taskmanager_test nyc mocha --require test/setup.js test/**/*.test.js --timeout 20000 --exit",
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/community": "^0.3.50",
    "@langchain/core": "^0.3.67",
    "@langchain/langgraph": "^0.3.11",
    "bcrypt": "^6.0.0",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fuse.js": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongodb": "^6.16.0",
    "natural": "^6.12.0",
    "nodemon": "^3.1.10",
    "openai": "^5.13.1",
    "socket.io": "^4.8.1"
  },
  "description": "",
  "devDependencies": {
    "chai": "^4.5.0",
    "chai-http": "^5.1.2",
    "cross-env": "^7.0.3",
    "mocha": "^11.6.0",
    "nyc": "^17.1.0",
    "supertest": "^7.1.1"
  },
  "nyc": {
    "include": [
      "routes/**/*.js",
      "models/**/*.js",
      "middleware/**/*.js",
      "utils/**/*.js",
      "services/**/*.js",
      "*.js"
    ],
    "exclude": [
      "test/**/*.js",
      "node_modules/**",
      "insertUser.js"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "check-coverage": true,
    "lines": 70,
    "functions": 70,
    "branches": 70,
    "statements": 70
  }
